# BEGIN_KITTY_THEME
# One Half Dark
include current-theme.conf
# END_KITTY_THEME
#

background_opacity 0.5
#background #1e1e1e
cursor_shape     block
enable_audio_bell no
font_size        14.0
shell_integration  no-cursor

# BEGIN_KITTY_FONTS
font_family      family="Fira Code"
bold_font        auto
italic_font      auto
bold_italic_font auto
# END_KITTY_FONTS
